{
    "name": "aferalabs/arachne",
    "description": "Behat extension for testing and validating XML and JSON based web services",
    "version": "0.5.0",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Wojtek Gancarczyk",
            "email": "wojtek@aferalabs.com",
            "homepage": "http://aferalabs.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "behat/behat": "~3.3.0",
        "guzzlehttp/guzzle": "~6.0",
        "justinrainbow/json-schema": "~4.1|~5.0",
        "seromenho/xml-validator": "~v1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Arachne\\": "src/Arachne/",
            "Arachne\\Mocks\\": "tests/Arachne/Mocks/",
            "Arachne\\Tests\\": "tests/Arachne/Tests/"
        }
    }
}